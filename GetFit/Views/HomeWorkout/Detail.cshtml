@model Home

<div>
    <img height="200" width="200" src="@Model.Image"/>
    <h2>@Model.Title</h2>
    <p>@Model.Description</p>
    <ul>
        <li>@Model.Address.Street</li>
        <li>@Model.Address.City</li>
        <li>@Model.Address.State</li>
        <li>@Model.HomeCategory</li>
    </ul>
</div>